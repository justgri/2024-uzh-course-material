    "# Make n shift matrix\n",
    "def make_shift_mat(n):\n",
    "    # Initialize an n x n matrix of zeros\n",
    "    S_n = np.zeros((n, n), dtype=int)\n",
    "    \n",
    "    # Fill the superdiagonal with 1s\n",
    "    S_n[np.arange(n-1), np.arange(1, n)] = 1\n",
